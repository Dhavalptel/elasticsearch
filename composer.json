{
  "name": "simple/elasticsearch",
  "description": "A simple package for elasticsearch",
  "keywords": ["Laravel", "Scout", "Elasticsearch", "Elastic", "search"],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "8.0.*||8.1.*||8.2.*",
    "elasticsearch/elasticsearch": "^7.11.2",
    "illuminate/support": "^9.0||^10.0",
    "laravel/scout": "^9.0||^10.0"
  },
  "autoload": {
    "psr-4": {
      "Simple\\ElasticSearch\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Simple\\ElasticSearch\\SimpleServiceProvider"
      ]
    }
  }
}
